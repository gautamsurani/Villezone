<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.villezone.gautam.fragments.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home">
    </fragment>

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.villezone.gautam.fragments.MyOrdersFragment"
        android:label="@string/my_orders"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_notification"
        android:name="com.villezone.gautam.fragments.NotificationFragment"
        android:label="@string/notification"
        tools:layout="@layout/fragment_notification" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.villezone.gautam.fragments.MyAccountFragment"
        android:label="@string/my_account"
        tools:layout="@layout/fragment_slideshow" />

    <fragment
        android:id="@+id/nav_change_password"
        android:name="com.villezone.gautam.fragments.ChangePasswordFragment"
        android:label="@string/change_password"
        tools:layout="@layout/fragment_change_password" />

    <fragment
        android:id="@+id/nav_faq"
        android:name="com.villezone.gautam.fragments.FAQFragment"
        android:label="@string/faq"
        tools:layout="@layout/fragment_f_a_q" />

    <fragment
        android:id="@+id/nav_faq_only"
        android:name="com.villezone.gautam.fragments.FAQOnlyFragment"
        android:label="@string/faq_only"
        tools:layout="@layout/fragment_f_a_q_only" />

    <fragment
        android:id="@+id/nav_about"
        android:name="com.villezone.gautam.fragments.AboutUsFragment"
        android:label="@string/faq_about"
        tools:layout="@layout/fragment_about_us" />



</navigation>